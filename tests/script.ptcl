prototype function printf(content: character*, ...): integer
typedata statement(statement_type: integer)

type string: character*

syntax multiple [left: static integer] [right: static integer] end {
	left * right
}

syntax test [T: static type]([value: integer]) end {
	function ~returned(): !T {
		~variable := value
		return ~variable
	}

	~returned()
}

function main(): integer {
	value := multiple 2 4
	test >integer(16)
	test >float(16)
	return 0
}